{
  "name": "i18next-wc",
  "version": "1.0.0-0",
  "description": "i18next web components",
  "keywords": [
    "web-component",
    "i18n",
    "i18next"
  ],
  "homepage": "https://github.com/spurreiter/i18next-wc#readme",
  "bugs": {
    "url": "https://github.com/spurreiter/i18next-wc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spurreiter/i18next-wc.git"
  },
  "license": "MIT",
  "author": "spurreiter",
  "main": "dist/index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "all": "npm-run-all clean build test",
    "build": "webpack -p --mode production",
    "clean": "rimraf dist",
    "dev": "npm-run-all -p dev:wct dev:webpack",
    "dev:wct": "npm t -- -p",
    "dev:webpack": "webpack --mode development -w",
    "prepublishOnly": "npm-run-all clean build",
    "readme": "markedpp -i README.md -o README.md",
    "start": "webpack-dev-server --mode development",
    "test": "wct --npm"
  },
  "dependencies": {},
  "devDependencies": {
    "apprun": "^2.23.10",
    "i18next": "^19.3.4",
    "i18next-icu": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "wct-browser-legacy": "^1.0.2",
    "web-component-tester": "^6.9.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
